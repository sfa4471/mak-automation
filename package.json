{
  "name": "mak-automation",
  "version": "1.0.0",
  "description": "MAK Lone Star Consulting Field Report Automation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "reset-password": "node reset-password.js",
    "migrate-db": "node server/migrate-database.js",
    "fix-status-constraint": "node server/fix-status-constraint.js",
    "setup-mobile": "node setup-mobile-env.js",
    "supabase:migrate": "node scripts/run-supabase-migration.js",
    "supabase:verify": "node scripts/verify-supabase-tables.js",
    "supabase:migrate-data": "node scripts/migrate-data-sqlite-to-supabase.js",
    "supabase:setup": "node setup-supabase-env.js",
    "supabase:verify-connection": "node verify-supabase.js",
    "supabase:execute-and-verify": "node scripts/execute-and-verify-migration.js",
    "migrate:correction-factor": "node scripts/run-correction-factor-migration.js",
    "check-project-counters": "node scripts/check-project-counters.js",
    "validate-credentials": "node scripts/validate-supabase-credentials.js",
    "verify-render": "node scripts/verify-render-supabase-deployment.js",
    "pre-deploy-check": "node scripts/pre-deployment-check.js",
    "render-setup": "node scripts/add-to-render-instructions.js",
    "qa:database": "node scripts/qa-database-verification.js",
    "create-admin": "node scripts/create-admin-user.js"
  },
  "keywords": [],
